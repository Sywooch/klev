{
    "name": "yiisoft/yii2-app-basic",
    "description": "Yii 2 Basic Project Template",
    "keywords": ["yii2", "framework", "basic", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.5",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "budyaga/yii2-users": "*",
        "kartik-v/yii2-widget-datepicker": "@dev",
        "zelenin/yii2-sms_ru": "dev-master",
        "kartik-v/yii2-widget-fileinput": "@dev",
        "mihaildev/yii2-ckeditor": "*",
        "2amigos/yii2-ckeditor-widget": "^2.0",
        "bariew/yii2-page-cms-module": "*",
        "bupy7/yii2-pages": "*",
        "kartik-v/yii2-tree-manager": "@dev",
        "ekaragodin/yii-maintenance-mode": "dev-master",
        "phpoffice/phpexcel": "^1.8",
        "kartik-v/yii2-widget-typeahead": "*",
        "kartik-v/yii2-widget-select2": "@dev",
        "2amigos/yii2-highcharts-widget": "~1.0",
        "philippfrenzel/yii2fullcalendar": "*",
        "guzzlehttp/guzzle": "~6.0",
        "electrolinux/phpquery": "^0.9.6",
        "darkdrim/yii2-simplehtmldom": "1.0",
        "keltstr/yii2-simplehtmldom": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ],
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
